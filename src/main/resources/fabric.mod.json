{
	"schemaVersion": 1,
	"id": "oneworldfolder",
	"version": "${version}",
	"name": "OneWorldFolder",
	"description": "This mod always you to launch worlds from the default .minecraft world (saves) directory!",
	"authors": [
		"LinusDev"
	],
	"contact": {
		"homepage": "https://linusdev.de/",
		"sources": "https://github.com/lni-dev/oneworldfolder"
	},
	"license": "Apache License 2.0",
	"icon": "assets/oneworldfolder/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			"de.linusdev.OneWorldFolderModClient"
		]
	},
	"mixins": [
		{
			"config": "oneworldfolder.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}